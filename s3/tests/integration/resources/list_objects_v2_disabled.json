{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyListObjectsV2OnSpecificPath",
      "Effect": "Deny",
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::nobucket",
      "Condition": {
        "StringLike": {
          "s3:prefix": "nopath"
        }
      }
    },
    {
      "Sid": "AllowAllOtherS3Actions",
      "Effect": "Allow",
      "Action": "s3:*",
      "Resource": "*"
    }
  ]
}
